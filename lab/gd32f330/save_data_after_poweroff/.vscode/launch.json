{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Remote Debug",
      "type": "cortex-debug",
      "request": "launch",
      "executable": "${workspaceRoot}/build/flash_test.elf",
      "cwd": "${workspaceRoot}",
      "servertype": "external",
      "interface": "swd",
      "gdbTarget": "localhost:2331",
      "armToolchainPath": "/opt/homebrew/bin/",
      "preLaunchCommands": [
        "make r_flash",
      ]
    }
  ]
}